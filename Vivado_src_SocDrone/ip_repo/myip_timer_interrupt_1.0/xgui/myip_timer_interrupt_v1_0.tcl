# Definitional proc to organize widgets for parameters.
proc init_gui { IPINST } {
  ipgui::add_param $IPINST -name "Component_Name"
  #Adding Page
  set Page_0 [ipgui::add_page $IPINST -name "Page 0"]
  ipgui::add_param $IPINST -name "C_S00_AXI_DATA_WIDTH" -parent ${Page_0} -widget comboBox
  ipgui::add_param $IPINST -name "C_S00_AXI_ADDR_WIDTH" -parent ${Page_0}
  ipgui::add_param $IPINST -name "C_S00_AXI_BASEADDR" -parent ${Page_0}
  ipgui::add_param $IPINST -name "C_S00_AXI_HIGHADDR" -parent ${Page_0}

  #Adding Page
  set Interval [ipgui::add_page $IPINST -name "Interval"]
  set_property tooltip {Edit timer interval} ${Interval}
  set INTRPT_INTERVAL_US [ipgui::add_param $IPINST -name "INTRPT_INTERVAL_US" -parent ${Interval}]
  set_property tooltip {edit timer interval in us} ${INTRPT_INTERVAL_US}


}

